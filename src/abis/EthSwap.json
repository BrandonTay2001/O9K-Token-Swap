  "source": "pragma solidity ^0.5.0;\r\n\r\nimport \"./Token.sol\";\r\n\r\ncontract EthSwap {\r\n    string public name = \"Token Swap!\";\r\n    Token public token;\r\n\r\n    constructor(Token _token) public {\r\n        token = _token;\r\n    }\r\n\r\n    // 1 ether = 0.1 O9K\r\n    // function ethToO9K(uint num) public returns(uint) {\r\n    //     return (num * 10);\r\n    // }\r\n\r\n    function purchaseTokens() public payable {\r\n        token.transfer(msg.sender, (msg.value * 10));\r\n    }\r\n}",
